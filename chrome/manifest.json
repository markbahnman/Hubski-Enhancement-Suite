{
    "name": "Hubski Enhancement Suite",
    "version": "0.3",
    "manifest_version": 2,
    "minimum_chrome_version": "20.0",
    "description": "Hubski Enhancement Suite",
    "content_scripts": [
    {
        "matches": [
            "http://hubski.com/*",
            "https://hubski.com/*",
            "http://*.hubski.com/*",
            "https://*.hubski.com/*"
        ],
        "js": [
            "jquery-1.8.3.min.js",
            "hubski_enhancement_suite.user.js"
        ],
        "run_at": "document_end"
    }],
    "permissions": [
        "http://hubski.com/*",
        "https://hubski.com/*",
        "http://*.hubski.com/*",
        "https://*.hubski.com/*"
    ]
}